{
  "name": "@bridge/head",
  "type": "module",
  "scripts": {
    "prebuild": "rm -rf build",
    "build": "tsc",
    "start": "node --env-file=.env build/index.js",
    "start:dev": "tsc-watch --onSuccess \"node --env-file=.env --inspect build/index.js\""
  },
  "types": "dist/index.d.ts",
  "imports": {
    "#src/*": "./build/*",
    "#root/*": "./"
  },
  "volta": {
    "node": "22.8.0",
    "npm": "10.8.2"
  },
  "dependencies": {
    "@bridge/bus": "^1.0.0",
    "@bridge/contract": "^1.0.0"
  }
}
